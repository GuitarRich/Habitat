<?xml version="1.0" encoding="utf-8"?>

<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
  <sitecore>
    <solutionFramework>
      <indexing>
        <providers>
          <add name="news" type="Sitecore.Feature.News.Indexing.NewsIndexContentProvider, Sitecore.Feature.News"
               patch:before="add[@name='fallback']" />
        </providers>
      </indexing>
    </solutionFramework>
    <pipelines>
      <initializeDependencyInjection>
        <processor type="Sitecore.Feature.News.Pipelines.InitializeDependencyInjection.RegisterServices, Sitecore.Feature.News" />
      </initializeDependencyInjection>
    </pipelines>
  </sitecore>
</configuration>